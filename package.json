{
  "name": "fastify-marketplace-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "format": "prettier --config prettier.config.cjs --write \"src/**/*.ts\"",
    "pr:precheck": "npm run format && npm run lint && npm run type-check && npm run build",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "start": "node build/index.js",
    "dev": "TZ=Europe/Berlin ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "test": "jest --silent",
    "test:detectOpenHandles": "jest --silent --detectOpenHandles",
    "db:push": "drizzle-kit push:mysql --config=drizzle.config.ts",
    "db:generate-migration": "drizzle-kit generate:mysql --config=drizzle.config.ts",
    "studio": "drizzle-kit studio"
  },
  "keywords": [],
  "author": "Florian Breuer",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^8.4.1",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "drizzle-kit": "^0.24.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "argon2": "^0.41.1",
    "dotenv": "^16.4.4",
    "drizzle-orm": "^0.29.3",
    "fastify": "^4.26.1",
    "fastify-guard": "^2.0.0",
    "i": "^0.3.7",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mysql2": "^3.9.1",
    "npm": "^10.8.3",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "uuid": "^10.0.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
